/**index.wxss**/

.user_img{
	width:140rpx;
	height:140rpx;
	border-radius:50%;
	overflow:hidden;
	margin:0px auto;
}
.date{
	height:90rpx;
	border-top:solid 1px #ccc;
	border-bottom:solid 1px #ccc;
	display:flex;
	align-items: center;
  	justify-content: center;
  	color:#666;
}
.date_left{
	height:70rpx;
}
.date_item{
	height:70rpx;
	border-right:solid 1px #ccc;
	width:50%;
	text-align:center;
	display:flex;
	align-items: center;
  	justify-content: center;
}
.date_item .text{
	height:50%;
}
.date_item image{
	width:17rpx;
	height:10rpx;
	margin-left:10rpx;
}
.team_list{
	padding:40rpx 20rpx 20rpx;
	border-bottom:solid 1px #ccc;
}
.team_item{
	height:78rpx;
	display:flex;
	align-items: center;
  	justify-content: center;
}
.team_left{
	width:14%;
	text-align:left;
}
.team_right{
	width:86%;
}
.team_left image{
	width:78rpx;
	height:78rpx;
}
.teat_add{
	color:#666;
}
.credit_list{
	padding:0 34rpx;
	height:110rpx;
	border-bottom:solid 1px #e0e0e0;
}
.credit_item{
	width:100%;
	height:110rpx;
	display:flex;
	align-items:center;
	justify-content:center;
}
.credit_reson{
	color:#444;
}
.credit_left{
	width:50%;
}
.credit_right{
	width:50%;
	text-align:right;
}
.credit{
	height:200rpx;
}
.credit_user{
	width:28%;
}
.credit_infor{
	width:40%;
}
.credit_number{
	width:32%;
}
.credit_cont{
	width:100%;
	height:160rpx;
}